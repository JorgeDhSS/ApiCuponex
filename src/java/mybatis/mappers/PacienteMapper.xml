<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="pacientes">
    <select id="login" resultType="pojos.Paciente" parameterType="pojos.Paciente">
        SELECT * FROM paciente WHERE usuario = #{usuario} AND password = #{password}
    </select>
</mapper>